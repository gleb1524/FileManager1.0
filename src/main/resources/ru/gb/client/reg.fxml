<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.HBox?>
<VBox alignment="TOP_LEFT" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.gb.client.RegController">
    <children>
        <TextField fx:id="test" visible="false"/>
        <TextField fx:id="login" promptText="Login"/>
        <PasswordField fx:id="password" promptText="Password"/>
        <TextField fx:id="name" promptText="Name"/>
        <TextField fx:id="surname" promptText="Surname"/>
        <HBox alignment="CENTER_LEFT">
            <Button fx:id="reg" text="Registration" onAction="#pressToRegistration"/>
            <Button fx:id="back" text="Back" onAction="#pressToBack"/>
        </HBox>

    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
